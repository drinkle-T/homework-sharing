/*编程判断任意一个正整数各位数字之和是奇数还是偶数。如果是奇数输出1，偶数输出0
*/
#include<stdio.h>
#include<math.h>
int main()
{
	int m,n,a,b,c,sum;
	scanf_s("%d", &m);
	b = m;
	sum = 0;
	while (b > 0)
	{
		c = b % 10;
		b = b / 10;
		sum = sum + c;
	
	}
	n = sum%2;

	if (n == 0)
	{
		printf("0");
	}
	else
	{
		printf("1");
	}
		
	return 0;
}
